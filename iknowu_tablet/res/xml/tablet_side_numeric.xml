<?xml version="1.0" encoding="UTF-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="@dimen/key_height"
    android:horizontalGap="0%p"
    android:verticalGap="5dp">
	
	<Row android:keyWidth="20%p">
		<Key android:codes="49" android:keyLabel="1" android:keyEdgeFlags="left"/>
		<Key android:codes="50" android:keyLabel="2" keyUprLongPressLabel="ABC" keyUprLongPressLabelCentered="true"/>
		<Key android:codes="51" android:keyLabel="3" keyUprLongPressLabel="DEF" keyUprLongPressLabelCentered="true"/>
		<Key android:codes="45" android:keyLabel="-" keyDarkBackground="true"/>
		<Key android:codes="43" android:keyLabel="+" android:keyEdgeFlags="right" keyDarkBackground="true"/>
	</Row>
      
	<Row android:keyWidth="20%p">
		<Key android:codes="52" android:keyLabel="4" keyUprLongPressLabel="GHI" keyUprLongPressLabelCentered="true" android:keyEdgeFlags="left"/>
		<Key android:codes="53" android:keyLabel="5" keyUprLongPressLabel="JKL" keyUprLongPressLabelCentered="true"/>
		<Key android:codes="54" android:keyLabel="6" keyUprLongPressLabel="MNO" keyUprLongPressLabelCentered="true"/>
		<Key android:codes="42" android:keyLabel="*" keyDarkBackground="true"/>
		<Key android:codes="47" android:keyLabel="/" android:keyEdgeFlags="right" keyDarkBackground="true"/>
	</Row>
	
	<Row android:keyWidth="20%p">
		<Key android:codes="55" android:keyLabel="7" keyUprLongPressLabel="PQRS" keyUprLongPressLabelCentered="true" android:keyEdgeFlags="left"/>
		<Key android:codes="56" android:keyLabel="8" keyUprLongPressLabel="TUV"  keyUprLongPressLabelCentered="true"/>
		<Key android:codes="57" android:keyLabel="9" keyUprLongPressLabel="WXYZ" keyUprLongPressLabelCentered="true"/>
		<Key android:codes="36" android:keyLabel="$" keyDarkBackground="true"/>
		<Key android:codes="61" android:keyLabel="=" android:keyEdgeFlags="right" keyDarkBackground="true"/>
	</Row>
	
	<Row android:keyWidth="20%p">
		<Key android:codes="-11" android:keyLabel="sym" android:popupKeyboard="@layout/popup_kbchoices_tablet" android:keyEdgeFlags="left" keyDarkBackground="true"/> 
		<Key android:codes="48" android:keyLabel="0"/>
		<Key android:codes="35" android:keyLabel="#" keyDarkBackground="true"/>
		<Key android:codes="40" android:keyLabel="(" keyDarkBackground="true"/>
		<Key android:codes="41" android:keyLabel=")" android:keyEdgeFlags="right" keyDarkBackground="true"/>
	</Row>
</Keyboard>